      <concern label="&urlbar;" minVersion="3.0">
        <pref key="browser.urlbar.default.behavior"
              type="int"
              minVersion="3.5"
              maxVersion="35.*">
          <behavior>
            <override name="setValue">
            <![CDATA[
              var checkboxes = view.getElementsByTagName('checkbox');
              for(var i = 0, l = checkboxes.length, checkbox; i < l; i++) {
                checkbox = checkboxes[i];
                checkbox.checked = parseInt(value) & parseInt(checkbox.getAttribute('value'));
              }
            ]]>
            </override>
            <override name="getValue">
            <![CDATA[
              var bitmap = 0;
              var checkboxes = view.getElementsByTagName('checkbox');
              for(var i = 0, l = checkboxes.length, checkbox; i < l; i++) {
                checkbox = checkboxes[i];
                if(checkbox.checked) {
                  bitmap += parseInt(checkbox.getAttribute('value'));
                }
              }
              return bitmap;
            ]]>
            </override>
          </behavior>
          <view>
            <group label="&urlbar_restrict_name;">
              <checkbox label="&browser_urlbar_restrict_typed_name;"
                        description="&browser_urlbar_restrict_typed_description;"
                        value="32" />
              <checkbox label="&browser_urlbar_restrict_history_name;"
                        description="&browser_urlbar_restrict_history_description;"
                        value="1" />
