<?xml version="1.0" ?>
<preferences>
	<panel label="accessibility">
		<tabs>
			<tab label="mouseclick">
				<group label="middleclick">
					<pref key="middlemouse.contentLoadURL" />
					<pref key="middlemouse.paste" />
				</group>
				<group label="urlbar">
					<pref key="browser.urlbar.clickSelectsAll" />
					<pref key="browser.urlbar.doubleClickSelectsAll" />
				</group>
			</tab>
			<tab label="urlbar">
				<pref key="browser.urlbar.autoFill" />
				<pref key="browser.urlbar.matchOnlyTyped" indent="indent" />
				<separator />
				<pref key="browser.fixup.alternate.enabled" />
				<pref key="browser.fixup.alternate.prefix" />
				<pref key="browser.fixup.alternate.suffix" />
			</tab>
			<tab label="awesomebar" minVersion="3.0">
				<pref key="browser.urlbar.maxRichResults" />
				<pref key="guiconfig.urlbar.restrict" minVersion="3.1a" config="Int" type="select">
					<option>-1</option>
					<option>0</option>
					<option>1</option>
					<option>2</option>
					<bind>
						<pref key="browser.urlbar.restrict.tag" config="Char" />
						<pref key="browser.urlbar.restrict.history" config="Char" />
						<pref key="browser.urlbar.restrict.bookmark" config="Char" />
					</bind>
				</pref>
				<pref key="guiconfig.urlbar.match" minVersion="3.1a" config="Int" type="select">
					<option>-1</option>
					<option>0</option>
					<option>1</option>
					<bind>
						<pref key="browser.urlbar.match.url" config="Char" />
						<pref key="browser.urlbar.match.title" config="Char" />
					</bind>
				</pref>
			</tab>
			<tab label="misc">
				<pref key="accessibility.tabfocus" type="select">
					<option>1</option>
					<option>2</option>
					<option>3</option>
					<option>4</option>
					<option>7</option>
				</pref>
				<pref key="browser.backspace_action" type="select">
					<option>0</option>
					<option>1</option>
					<option>2</option>
				</pref>
				<pref key="signon.prefillForms" config="Bool" default="1">
					<bind>
						<pref key="signon.autofillForms" config="Bool" />
					</bind>
				</pref>
				<pref key="browser.search.openintab" />
				<pref key="browser.preferences.instantApply" minVersion="2.0" />
				<pref key="browser.popups.showPopupBlocker" />
				<pref key="accessibility.warn_on_browsewithcaret" />
				<pref key="accessibility.accesskeycausesactivation" />
			</tab>
		</tabs>
	</panel>
	<panel label="browser">
		<tabs>
			<tab label="alerts">
				<pref key="update.showSlidingNotification" config="Bool" default="1">
					<bind>
						<pref key="update_notifications.enabled" config="Bool" default="1" />
					</bind>
				</pref>
				<pref key="browser.download.manager.showAlertOnComplete" />
				<pref key="browser.download.manager.showAlertInterval" indent="indent" />
				<separator />
				<pref key="alerts.slideIncrement" />
				<pref key="alerts.slideIncrementTime" />
				<pref key="alerts.totalOpenTime" />
			</tab>
			<tab label="cache">
				<pref key="browser.cache.check_doc_frequency" type="select">
					<option>0</option>
					<option>1</option>
					<option>2</option>
					<option>3</option>
				</pref>
				<separator />
				<pref key="browser.cache.disk.enable" />
				<pref key="browser.cache.disk.capacity" indent="indent" />
				<pref key="browser.cache.disk_cache_ssl" indent="indent" />
				<separator />
				<pref key="browser.cache.memory.enable" />
				<pref key="browser.cache.memory.capacity" indent="indent" config="Int" default="-1" />
			</tab>
			<tab label="extensions">
				<pref key="extensions.checkCompatibility" minVersion="2.0b1" config="Bool" default="1" />
				<group label="getaddons" minVersion="3.0">
					<pref key="extensions.getAddons.showPane" />
					<pref key="extensions.getAddons.maxResults" />
				</group>
			</tab>
			<tab label="downloadmanager">
				<pref key="browser.download.manager.focusWhenStarting" />
				<pref key="browser.download.manager.alertOnEXEOpen" config="Bool" default="1" />
				<pref key="browser.download.lastDir" config="Char" />
			</tab>
		</tabs>
	</panel>
	<panel label="style">
		<tabs>
			<tab label="browser_style">
				<pref key="browser.chrome.favicons">
					<bind>
						<pref key="browser.chrome.site_icons" />
					</bind>
				</pref>
				<pref key="browser.chrome.toolbar_tips" />
				<pref key="browser.urlbar.hideGoButton" minVersion="2.0" maxVersion="2.*" config="Bool" />
				<pref key="browser.preferences.animateFadeIn" />
				<pref key="browser.tabs.closeButtons" type="select" minVersion="2.0">
					<option>0</option>
					<option>1</option>
					<option>2</option>
					<option>3</option>
				</pref>
			</tab>
			<tab label="doc_style">
				<group label="rendering">
					<pref key="nglayout.initialpaint.delay" config="Int" default="250" />
					<pref key="content.notify.interval" config="Int" default="120000" />
				</group>
				<pref key="browser.display.show_image_placeholders" />
				<pref key="browser.enable_automatic_image_resizing" />
				<pref key="browser.frames.enabled" />
				<pref key="browser.blink_allowed" />
			</tab>
			<tab label="text_select">
				<pref key="ui.textSelectBackground" type="colorpicker" config="Char" />
				<pref key="ui.textSelectForeground" type="colorpicker" config="Char" />
				<pref key="ui.textSelectBackgroundAttention" type="colorpicker" config="Char" />
			</tab>
		</tabs>
	</panel>
	<panel label="bookmarks">
		<tabs>
			<tab label="microsummaries" minVersion="2.0">
				<pref key="browser.microsummary.enabled" />
				<pref key="browser.microsummary.updateInterval" config="Int" />
				<pref key="browser.microsummary.updateGenerators" />
				<pref key="browser.microsummary.generatorUpdateInterval" config="Int" />
			</tab>
			<tab label="misc">
				<pref key="browser.tabs.loadBookmarksInBackground" />
				<pref key="browser.bookmarks.livemark_refresh_seconds" config="Int" default="3600" />
				<pref key="browser.bookmarks.max_backups" minVersion="2*" />
			</tab>
		</tabs>
	</panel>
	<panel label="network">
		<tabs>
			<tab label="connections">
				<group label="connections">
					<pref key="network.http.max-connections" />
					<pref key="network.http.max-connections-per-server" />
				</group>
				<group label="keep-alive_connections">
					<pref key="network.http.keep-alive" />
					<pref key="network.http.max-persistent-connections-per-server" />
					<pref key="network.http.max-persistent-connections-per-proxy" />
				</group>
			</tab>
			<tab label="pipelining">
				<pref key="network.http.pipelining" />
				<pref key="network.http.proxy.pipelining" />
				<pref key="network.http.pipelining.maxrequests" />
			</tab>
		</tabs>
	</panel>
	<panel label="developing">
		<group label="viewsource">
			<pref key="view_source.editor.external" />
			<pref key="view_source.editor.path" type="file" indent="indent">
				<filter>Apps</filter>
			</pref>
		</group>
		<group label="misc">
			<pref key="browser.dom.window.dump.enabled" config="Bool" />
			<pref key="javascript.options.strict" config="Bool" />
			<pref key="javascript.options.showInConsole" />
			<pref key="nglayout.debug.disable_xul_cache" config="Bool" />
		</group>
	</panel>
</preferences>